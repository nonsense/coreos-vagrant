[Unit]
Description=Redis cluster 1 node data
After=docker.service
Requires=docker.service

[Service]
Environment=UNIT=%p
TimeoutStartSec=0
ExecStart=/usr/bin/docker run --name ${UNIT} \
	-v /data \
	172.17.8.1:5000/busybox \
	/bin/sh -c 'chown 999 /data && echo volume container: ${UNIT}'

[X-Fleet]
Global=true
